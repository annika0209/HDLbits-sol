module top_module (
    input clk,
    input reset,
    output [3:0] q);
    
    always @(posedge clk) begin
        if(reset) begin
           q <= 4'b1; 
        end
        else if(q==4'd10) begin
           	q <= 4'b1;
        end
    	else begin 
            q <= q + 4'b1;
        end
    end
endmodule
=======
module top_module (
    input clk,
    input reset,
    output [3:0] q);
    
    always @(posedge clk) begin
        if(reset) begin
           q <= 4'b1; 
        end
        else if(q==4'd10) begin
           	q <= 4'b1;
        end
    	else begin 
            q <= q + 4'b1;
        end
    end
endmodule
